{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./out",

    "strict": true,
    "strictPropertyInitialization": true,
    "noImplicitAny": true,
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules"
  ]
}
